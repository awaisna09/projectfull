# Grading System Configuration
# This file contains all the necessary API keys and configuration for the AI grading system

# OpenAI API Configuration
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# LangSmith API Configuration for Tracing and Monitoring
LANGSMITH_TRACING="true"
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="YOUR_LANGSMITH_API_KEY_HERE"
LANGSMITH_PROJECT="grading"

# Grading System Configuration
GRADING_MODEL="gpt-5-nano-2025-08-07"
GRADING_TEMPERATURE="0.1"
GRADING_MAX_TOKENS="4000"

# Server Configuration
GRADING_HOST="0.0.0.0"
GRADING_PORT="8001"

# Logging Configuration
LOG_LEVEL="INFO"
ENABLE_DEBUG="true"

# Performance Configuration
REQUEST_TIMEOUT="30"
MAX_CONCURRENT_REQUESTS="10"
